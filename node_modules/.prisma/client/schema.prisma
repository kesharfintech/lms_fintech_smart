model User {
  id        Int       @id @default(autoincrement())
  name      String
  email     String    @unique
  password  String
  createdAt DateTime  @default(now())
  createdBy String?
  updateBy  String?
  updateAt  DateTime?
  menus     Menu[]
}

model Menu {
  id        Int       @id @default(autoincrement())
  menuName  String    @unique
  userId    Int
  user      User      @relation(fields: [userId], references: [id])
  createdAt DateTime  @default(now())
  createdBy String
  updateBy  String?
  updateAt  DateTime?
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
