generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Menu {
  id       Int    @id @default(autoincrement())
  menuName String @unique

  userId Int?

  createdBy String // sirf name store hoga
  updateBy  String?
  updateAt  DateTime?

  createdAt DateTime @default(now())
}
